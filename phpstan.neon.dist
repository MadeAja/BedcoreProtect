parameters:
    level: 7
    checkMissingIterableValueType: false
    paths:
        - /source/src
    autoload_files:
        - phar:///pocketmine/PocketMine-MP.phar/vendor/autoload.php
    autoload_directories:
        - /source/src
        - phar:///source/vendor/libasynql.phar/src
        - phar:///source/vendor/UpdateNotifier.phar/src
        - phar:///source/vendor/FormLib.phar/src
        - phar:///source/vendor/await-generator.phar/src
        - phar:///source/vendor/BlockSniper.phar/src
    excludes_analyse:
        - /source/vendor
        - /source/src/matcracker/BedcoreProtect/commands/CommandParser.php
        - /source/src/matcracker/BedcoreProtect/storage/queries/QueriesBlocksTrait.php
        - /source/src/matcracker/BedcoreProtect/storage/queries/QueriesEntitiesTrait.php
        - /source/src/matcracker/BedcoreProtect/storage/queries/QueriesInventoriesTrait.php
    ignoreErrors:
        -
            message: "#^Parameter \\#1 $data of static method pocketmine\\\\level\\\\format\\\\Chunk::fastDeserialize() expects string, pocketmine\\\\level\\\\format\\\\Chunk|string given.$#"
            count: 1
            path: /source/src/matcracker/BedcoreProtect/tasks/async/AsyncRollbackTask.php